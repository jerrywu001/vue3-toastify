{
  "name": "vue3-toastify",
  "version": "0.0.0",
  "description": "ðŸŽ‰ Vue3-Toastify allows you to add notifications to your app with ease. No more nonsense!",
  "keywords": [
    "vue3",
    "vue3-toastify",
    "toast",
    "notification",
    "alert",
    "vue3-component"
  ],
  "homepage": "https://github.com/jerrywu001/vue3-toastify#readme",
  "bugs": {
    "url": "https://github.com/jerrywu001/vue3-toastify/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jerrywu001/vue3-toastify.git"
  },
  "license": "MIT",
  "author": "jerrywu001 <57242263@163.com>",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "src/component.d.ts",
    "package.json",
    "README.md"
  ],
  "workspaces": [
    "playground"
  ],
  "scripts": {
    "build": "tsup && npm run build:css",
    "build:css": "postcss src/styles/index.css -o dist/index.css",
    "build:css-watch": "postcss src/styles/index.css -o dist/index.css -w",
    "build:storybook": "npm run build:storybook -w playground",
    "build:vite": "vite build && npm run build:css",
    "dev": "concurrently \"npm run build:css-watch\" \"npm run dev -w playground\"",
    "dev:pack": "npm run dev:usepack -w playground",
    "postinstall": "rimraf rf ./node_modules/@types/react",
    "lint": "eslint --ext .ts,.tsx,.vue ./src",
    "prepare": "chmod a+x .husky/* && husky install",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --tag --push",
    "storybook": "npm run storybook -w playground",
    "stylelint": "stylelint **/*.{css,less,scss,vue}",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc && vue-tsc"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/plugin-transform-typescript": "^7.18.12",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@storybook/addon-actions": "~6.4.22",
    "@storybook/addon-docs": "~6.4.22",
    "@storybook/addon-essentials": "~6.4.22",
    "@storybook/addon-links": "~6.4.22",
    "@storybook/builder-vite": "0.1.37",
    "@storybook/vue3": "~6.4.22",
    "@swc/core": "^1.2.242",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^6.6.1",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lz-string": "^1.3.34",
    "@types/node": "^18.7.11",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vitest/ui": "^0.22.1",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/eslint-config-typescript": "^11.0.0",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "bumpp": "^8.2.1",
    "concurrently": "^7.3.0",
    "core-js": "^3.24.1",
    "cssnano": "^5.1.13",
    "esbuild-plugin-babel": "^0.2.3",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-vue": "^9.4.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "npm": "^8.18.0",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "postcss-loader": "^7.0.1",
    "postcss-nested": "^5.0.6",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-prettier": "^2.0.0",
    "tsup": "^6.2.2",
    "typescript": "^4.7.4",
    "vite": "^3.0.9",
    "vite-plugin-dts": "^1.4.1",
    "vite-plugin-windicss": "^1.8.7",
    "vitest": "^0.22.1",
    "vue-tsc": "^0.40.1"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  }
}
